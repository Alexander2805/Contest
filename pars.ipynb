    "# result = list(map(process_page, tqdm(all_links)))\n",
    "with Pool(processes=10) as pool:\n",
    "    result = pool.map(process_page, all_links) \n",
    "df = pd.DataFrame(result)\n",
    "df.sort_values(by=['ID'], inplace=True)\n",
    "df = df.fillna('')\n",
    "\n",
    "with open('parsing.csv', mode='w', encoding='utf-8') as f_csv:\n",
    "    df.to_csv(f_csv, index=False)"
   ]
