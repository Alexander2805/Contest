    "def process_page(url):\n",
    "    table = dict()\n",
    "    plug = ''\n",
    "    page_html = requests.get(url).text\n",
    "    soup = BeautifulSoup(page_html, 'html.parser')\n",
    "    block1 = soup.find('div', class_=\"rd-page-product__row\")\n",
    "    block2 = soup.find('div', class_=\"rd-page-product__row rd-page-product__desc\")\n",
    "\n",
    "    article = block1.find('span', itemprop=\"sku\")\n",
    "    table['ID'] = article.text\n",
    "\n",
    "    table['URL'] = url\n",
    "\n",
    "    name_of_book = block1.find('h1', class_=\"rd-page-product__title\")\n",
    "    table['Название'] = name_of_book.text\n",
    "    \n",
    "    name = block1.find('a', itemprop=\"brand\")\n",
    "    if name is not None:\n",
    "        name = re.sub(r' и', ';', name.text)\n",
    "    else: name = plug\n",
    "    table['Автор'] = name\n",
    "    \n",
    "    path = block1.find('a', class_=\"download-pdf\")\n",
    "    if path is not None:\n",
    "        path = 'https://www.respublica.ru' + path['href']\n",
    "    else: name = plug\n",
    "    table['Превью'] = path\n",
    "    \n",
    "    img = soup.find_all('link')\n",
    "    img = img[7]['href']\n",
    "    table['Изобрвжение'] = img\n",
    "    \n",
    "    description = block2.find('div', class_=\"rd-page-product__desc-body\")\n",
    "    table['Описание'] = description.text\n",
    "    \n",
    "    price = block1.find('span', class_=\"num\")\n",
    "    price = re.sub(r'\\D', '', price.text)\n",
    "    table['Цена'] = price\n",
    "    \n",
    "    old_price = block1.find('span', class_=\"prev\")    \n",
    "    if old_price is not None:  \n",
    "        old_price = re.sub(r'\\D', '', old_price.text)\n",
    "    else: name = plug\n",
    "    table['Цена (старая)'] = old_price\n",
    "    \n",
    "    stock = block1.find('option', selected=\"selected\")\n",
    "    if stock is not None:\n",
    "        stock = True\n",
    "    else:\n",
    "        stock = False\n",
    "    table['В наличии'] = stock\n",
    "    \n",
    "    reviews = block1.find('span', itemprop=\"aggregateRating\")   \n",
    "    if reviews is not None:\n",
    "        reviews = reviews.find_all('meta')\n",
    "        rating, reviews_number, ratings_number = \\\n",
    "        reviews[3]['content'], reviews[0]['content'], reviews[1]['content']\n",
    "    else:\n",
    "        rating, reviews_number, ratings_number = plug, plug, plug\n",
    "    table['Число отзывов'] = reviews_number\n",
    "    table['Число оценок'] = ratings_number\n",
    "    table['Оценка'] = rating\n",
    "    \n",
    "    min_table = block2.find('div', class_=\"rd-page-product__desc-params\")\n",
    "    min_table = min_table.find_all('p', class_=\"rd-page-product__desc-param\")\n",
    "    min_table = list(map(lambda x: x.text.split(':'), min_table))\n",
    "    for row in min_table:\n",
    "        table[row[0]] = row[1].strip()\n",
    "    \n",
    "    return table"
   ]
  },
