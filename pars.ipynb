   "source": [
    "from multiprocessing import Pool\n",
    "from tqdm.notebook import tqdm\n",
    "from bs4 import BeautifulSoup\n",
    "import pandas as pd\n",
    "import numpy as np\n",
    "import requests\n",
    "import re"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 2,
   "metadata": {
    "ExecuteTime": {
     "end_time": "2020-04-16T08:42:14.709144Z",
     "start_time": "2020-04-16T08:42:14.701661Z"
    }
   },
   "outputs": [],
   "source": [
    "link = 'https://www.respublica.ru/authors/'\n",
    "link_on_author = list()\n",
    "with open('instruction/authors.txt') as f:\n",
    "    for num, author in enumerate(f):\n",
    "        author = re.sub(r'\\n', '', author)\n",
    "        link_on_author.append(link + author + '?page={}')"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 3,
   "metadata": {
    "ExecuteTime": {
     "end_time": "2020-04-16T08:42:14.721697Z",
     "start_time": "2020-04-16T08:42:14.714228Z"
    }
   },
   "outputs": [],
   "source": [
    "def links_of_page(link):\n",
    "    page_html = requests.get(link).text\n",
    "    soup = BeautifulSoup(page_html, 'html.parser')\n",
    "    links_with_dirt = soup.find_all('a', class_=\"rd-listing-product-item__image-wrapper\")\n",
    "    _links = list(map(lambda x: x['href'], links_with_dirt))\n",
    "    links = [ 'https://www.respublica.ru' + i for i in _links ]\n",
    "    return links"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 4,
   "metadata": {
    "ExecuteTime": {
     "end_time": "2020-04-16T08:42:14.736138Z",
     "start_time": "2020-04-16T08:42:14.727781Z"
    }
   },
   "outputs": [],
   "source": [
    "def links_on_books_of_author(link_on_author):\n",
    "    links_of_authors = list()\n",
    "    def _links_on_books_of_author(link_on_author, num_of_page=1):\n",
    "        page = links_of_page(link_on_author.format(num_of_page))\n",
    "        if len(page) == 0:\n",
    "            return links_of_authors\n",
    "        links_of_authors.extend(page)\n",
    "        return _links_on_books_of_author(link_on_author, num_of_page + 1)\n",
    "    return _links_on_books_of_author(link_on_author, num_of_page=1)"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 5,
   "metadata": {
    "ExecuteTime": {
     "end_time": "2020-04-16T08:42:28.757073Z",
     "start_time": "2020-04-16T08:42:14.739790Z"
    }
   },
   "outputs": [],
   "source": [
    "# all_links = list()    \n",
    "# for author in tqdm(link_on_author):\n",
    "#     all_links.extend( links_on_books_of_author(author) )\n",
    "with Pool(processes=10) as pool:\n",
    "    mass = pool.map(links_on_books_of_author, link_on_author)\n",
    "pool.join()"
    "all_links = [ element for row in mass for element in row ]"
   ]
